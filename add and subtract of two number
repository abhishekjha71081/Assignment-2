#include <stdio.h>
#include <math.h>

int main()
{
	int a,b;
    float c,d;
    scanf("%d %d",&a,&b);
    
    scanf("%f %f",&c,&d);
    /*add and subtract two integer number*/
    printf("%d %d",a+b,a-b);
    printf("\n");
    /*add and subtract of float number*/
    printf("%.1f %.1f",c+d,c-d);
    return 0;
}
